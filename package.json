    "css-loader": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.27.1",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
